<main class="container">
  <%= link_to sanitize("&larr; Back to #{@quote.name}"), quote_path(@quote) %>

  <div class="header">
    <h1>Edit date</h1>
  </div>
  <%= turbo_frame_tag dom_id(@line_item_date, :edit) do %>
    <%= render "date_form", quote: @quote, line_item_date: @line_item_date %>
  <% end %>
</main>
